C++ version:
class Solution {
public:
    void moveZeroes(vector<int>& nums) {
        int left=0;
        
        for(int i=0; i<nums.size(); ++i){
            if(nums[i]){
                swap(nums[left], nums[i]);
                left++;
            }
        }
        
        return;
        
    }
};
